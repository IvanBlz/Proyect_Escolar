{{#each Users}}
<div class="card mb-3 mx-auto">
  <img src="img/user/{{Nick}}.jpeg" style="width: 20%;" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{Nick}}</h5>
    <p class="card-text">{{User_Name}}</p>
    <p class="card-text"><small class="text-muted">{{Email}}</small></p>
  </div>
</div>
{{/each}}